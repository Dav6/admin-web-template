{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["auto-imports.d.ts","env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noImplicitThis": false,

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
